<?xml version="1.0"?>
<package>
  <metadata>
    <id>{package_name}</id>
    <version>{version}</version>
    <authors>gissehel</authors>
    <owners>gissehel</owners>
    <projectUrl>https://github.com/gissehel/BarLauncher-EasyHelper</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A bar launcher helper to create plugins for bar launcher apps like Wox or Flow Launcher.{description_add}</description>
    <releaseNotes></releaseNotes>
    <copyright>MIT</copyright>
    <tags>Bar Launcher Plugin Helper</tags>
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="BarLauncher.EasyHelper" version="{version}" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="{package_path}\bin\Release\netstandard2.0\{package_name}.dll" target="lib\netstandard2.0" />
  </files> 
</package>
