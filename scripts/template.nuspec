<?xml version="1.0"?>
<package>
  <metadata>
    <id>{package_name}</id>
    <version>{version}</version>
    <authors>gissehel</authors>
    <owners>gissehel</owners>
    <projectUrl>https://github.com/gissehel/Wox-EasyHelper</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A Wox helper to create Wox plugins.{description_add}</description>
    <releaseNotes></releaseNotes>
    <copyright>MIT</copyright>
    <tags>Wox Plugin Helper</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.8">
        <dependency id="Wox.Plugin" version="[1.3.159,1.4)" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="{package_path}\Bin\Release\net48\{package_name}.dll" target="lib\net48\{package_name}.dll" />
  </files> 
</package>
